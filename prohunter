clear
echo -e $grn
echo -e $grn"SetUp Terminel By ProHunter"
echo -e $grn
rm -rf .zshrc .draw.sh .bashrc
echo -e $grn
cp .ProHunter/.bashrc ~/
cp .ProHunter/.zshrc ~/
cp .ProHunter/.draw.sh ~/
cp .ProHhnter/.zsh_history ~/
echo -e $grn
rm -rf ~/.termux/font.ttf colors.properties
echo -e $grn
cp .ProHunter/colors.properties ~/.termux
cp .ProHunter/font.ttf ~/.termux
echo -e $grn
chmod +x .bashrc
echo -e $grn
exit





